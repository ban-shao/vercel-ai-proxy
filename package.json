{
  "name": "vercel-ai-proxy",
  "version": "1.1.0",
  "description": "Vercel AI Gateway 代理服务 - 使用官方 AI SDK Gateway，支持密钥池管理、余额检查、定时刷新",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "check": "tsx src/cli/check.ts",
    "refresh": "tsx src/cli/refresh.ts",
    "daily-task": "tsx src/cli/daily-task.ts"
  },
  "dependencies": {
    "@ai-sdk/gateway": "^1.0.0",
    "ai": "^5.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
